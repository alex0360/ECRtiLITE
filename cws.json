  "paths": {
    "/afg/startDate/{StartDate}/endDate/{EndDate}/query/{Query}/pageSize/{PageSize}": {
      "get": {
        "tags": [
          "Transactions"
        ],
        "summary": "Permite obtener las transacciones de los comercios que participan en el servicio de Fidelium",
        "parameters": [
          {
            "name": "StartDate",
            "in": "path",
            "description": "Fecha donde se debe iniciar la consulta.",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string",
              "example": "05-21"
            }
          },
          {
            "name": "EndDate",
            "in": "path",
            "description": "Fecha hasta donde llegara la consulta.",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string",
              "example": "05-21"
            }
          },
          {
            "name": "Query",
            "in": "path",
            "description": "Filtro.",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string",
              "example": "null"
            }
          },
          {
            "name": "PageSize",
            "in": "path",
            "description": "Tamaño de la pagina.",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string",
              "example": "10"
            }
          }
        ],
        "responses": {
          "200": {
